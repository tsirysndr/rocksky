amends  "../../schema/lexicon.pkl"

lexicon = 1
id = "app.rocksky.playlist.removeTrack"
defs = new Mapping<String, Procedure> {
  ["main"] {
    type = "procedure"
    description = "Remove a track from a playlist"
    parameters {
      type = "params"
      required = List("uri", "position")
      properties {
        ["uri"] = new StringType {
          type = "string"
          description = "The URI of the playlist to remove the track from"
          format = "at-uri"
        }
        ["position"] = new IntegerType {
          type = "integer"
          description = "The position of the track to remove in the playlist"
        }
      }
    }
  }
}