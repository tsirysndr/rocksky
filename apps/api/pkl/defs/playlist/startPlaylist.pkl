    type = "procedure"
    description = "Start a playlist"
    parameters {
      type = "params"
      required = List("uri")
      properties {
        ["uri"] = new StringType {
          type = "string"
          description = "The URI of the playlist to start"
          format = "at-uri"
        }
      }
    }
  }
}
