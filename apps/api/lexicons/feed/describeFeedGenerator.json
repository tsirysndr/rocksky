{
  "lexicon": 1,
  "id": "app.rocksky.feed.describeFeedGenerator",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get information about a feed generator",
      "parameters": {
        "type": "params",
        "properties": {}
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "properties": {
            "did": {
              "type": "string",
              "description": "The DID of the feed generator.",
              "format": "at-identifier"
            },
            "feeds": {
              "type": "array",
              "description": "List of feed URIs generated by this feed generator.",
              "items": {
                "type": "ref",
                "ref": "app.rocksky.feed.defs#feedUriView"
              }
            }
          }
        }
      }
    }
  }
}
