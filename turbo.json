{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "install": {
      "dependsOn": [
        "^install"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "./dist/**"
      ]
    },
    "build:prod": {
      "dependsOn": [
        "^build:prod"
      ],
      "outputs": [
        "./dist/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "prod": {
      "persistent": true,
      "cache": false
    },
    "sync": {
      "persistent": true,
      "cache": false
    },
    "genres": {
      "persistent": true,
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "pkl:eval": {
      "cache": false
    },
    "pkl:gen": {
      "cache": false
    },
    "prod:all": {
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "android": {
      "persistent": true,
      "cache": false
    },
    "ios": {
      "persistent": true,
      "cache": false
    },
    "storybook": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "format": {
      "dependsOn": [
        "^format"
      ],
      "outputs": []
    },
    "adb:reverse": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "./coverage/**"
      ]
    }
  }
}