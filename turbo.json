{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "install": {
      "dependsOn": [
        "^install"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "./dist/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "pkl:eval": {
      "cache": false
    },
    "pkl:gen": {
      "cache": false
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "android": {
      "persistent": true,
      "cache": false
    },
    "ios": {
      "persistent": true,
      "cache": false
    },
    "storybook": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "outputs": []
    },
    "adb:reverse": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "./coverage/**"
      ]
    },
    "dev:analytics": {
      "persistent": false,
      "cache": false
    },
    "dev:connect": {
      "persistent": false,
      "cache": false
    },
    "dev:dropbox": {
      "persistent": false,
      "cache": false
    },
    "dev:googledrive": {
      "persistent": false,
      "cache": false
    },
    "dev:jetstream": {
      "persistent": false,
      "cache": false
    },
    "dev:playlists": {
      "persistent": false,
      "cache": false
    },
    "dev:scrobbler": {
      "persistent": false,
      "cache": false
    },
    "dev:spotify": {
      "persistent": false,
      "cache": false
    },
    "dev:storage": {
      "persistent": false,
      "cache": false
    },
    "dev:webscrobbler": {
      "persistent": false,
      "cache": false
    }
  }
}